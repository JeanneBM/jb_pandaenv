            steps {
                sh &quot;mvn package -Pdocker&quot;
            }
        }

        stage(&apos;Run Docker app&apos;) {
            steps {
                sh &quot;docker run -d -p 0.0.0.0:8085:8085 --name pandaapp -t ${IMAGE}:${VERSION}&quot;
